// REQUIRES: instrumentation && fortran
// RUN: c_file=$(echo %s | sed 's/_instr.test/.f90/')
// RUN: %mpifort -g -S -emit-llvm "$c_file" -o %t.ll
// RUN: %parcoach -check=rma -o %t_instr.ll %t.ll
