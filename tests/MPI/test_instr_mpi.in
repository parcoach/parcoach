#!/usr/bin/env bash
set -euxo pipefail

if [ $# -ne 2 ]; then
  echo "Usage: ./test_instr_mpi executable expected_output.txt"
  exit
fi

EXE=$1
EXE_OUT=$1.out
EXPECTED_OUTPUT=$2

export HWLOC_HIDE_ERRORS=2
@MPI_RUN_BIN@ "${EXE}" >"${EXE_OUT}"
@Python3_EXECUTABLE@ @PARCOACH_FILE_CONTAINS@ ${EXE_OUT} ${EXPECTED_OUTPUT}
