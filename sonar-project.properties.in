sonar.host.url=https://sonarqube.inria.fr/sonarqube
sonar.projectKey=storm:parcoach:git
sonar.links.homepage=https://parcoach.github.io/
sonar.links.scm=https://gitlab.inria.fr/parcoach/parcoach
sonar.projectDescription=@CPACK_PACKAGE_DESCRIPTION_SUMMARY@
